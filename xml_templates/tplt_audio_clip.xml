							<clipitem id="{clipid}" premiereChannelType="{type}">
								<name>{clipname}</name>
								<enabled>TRUE</enabled>
								<duration>{audiolen}</duration>
								<rate>
									<timebase>{timebase}</timebase>
									<ntsc>{ntsc}</ntsc>
								</rate>
								<start>{start}</start>
								<end>{end}</end>
								<in>{in}</in>
								<out>{out}</out>
								<file id="{fileid}">
									<name>{filename}</name>
									<pathurl>{filepath}</pathurl>
									<rate>
										<timebase>{timebase}</timebase>
										<ntsc>{ntsc}</ntsc>
